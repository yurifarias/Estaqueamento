<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.yurifarias.estaqueamento.activities.CalcularEstaqueamentoActivity"
    tools:showIn="@layout/activity_calcular_estaqueamento"
    android:orientation="vertical">

    <Button
        android:id="@+id/reacoesNormais_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Reações Normais"/>

    <Button
        android:id="@+id/deslocamentosElasticos_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Deslocamentos Elásticos"/>

</LinearLayout>
